{
    "name": "evolution36/laravel-whoops-tracker",
    "description": "Packages for tracking all information about errors that occured in your application. Great for debugging errors in a production environment. Also provides a nice dashboard for displaying the errors.",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/config": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "illuminate/routing": "~5.5.0|~5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Evolution36\\WhoopsTracker\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evolution36",
            "email": "info@evolution36.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Evolution36\\WhoopsTracker\\WhoopsTrackerServiceProvider"
            ]
        }
    }
}
